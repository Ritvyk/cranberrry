{
  "name": "cranberrry",
  "version": "0.1.1",
  "description": "AI Agentic UI Framework For Frontend",
  "keywords": [
    "agentic",
    "ai",
    "artificial-intelligence",
    "agent",
    "react",
    "typescript",
    "frontend",
    "ui",
    "framework",
    "automation",
    "chatbot",
    "conversational-ai",
    "ai-agent",
    "react-hooks",
    "state-management",
    "typescript-library",
    "npm-package"
  ],
  "homepage": "https://github.com/Ritvyk/cranberrry#readme",
  "bugs": {
    "url": "https://github.com/Ritvyk/cranberrry/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ritvyk/cranberrry.git"
  },
  "license": "ISC",
  "author": "Ritvik",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit",
    "publish": "npm run clean && npm run build && npm publish"
  },
  "dependencies": {
    "mitt": "^3.0.1",
    "nanoid": "^5.1.5",
    "react": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "tsup": "^8.0.2",
    "typescript": "^5.4.3"
  }
}
